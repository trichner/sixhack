{
  "manifest_version": 2,

  "name": "CoffeeCup",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "0.1",

  "browser_action": {
    "default_icon": "res/coffee.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://api.qrserver.com/v1/",
    "storage"
  ]
}
